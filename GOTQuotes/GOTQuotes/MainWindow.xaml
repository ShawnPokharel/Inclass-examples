<Window x:Class="GOTQuotes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOTQuotes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="113*"/>
            <RowDefinition Height="306*"/>
        </Grid.RowDefinitions>
        <Image x:Name="imgGOTBackground" HorizontalAlignment="Left" Height="419" Margin="10,0,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="761"/>
        <Button x:Name="btnRandomQuote" Content="Random Quote" HorizontalAlignment="Left" Height="42" Margin="306,231,0,0" Grid.Row="1" VerticalAlignment="Top" Width="163" Click="BtnRandomQuote_Click"/>
        <ListBox x:Name="lstGOTQuote" HorizontalAlignment="Left" Height="199" Margin="43,10,0,43" Grid.Row="1" VerticalAlignment="Top" Width="693" Background="{x:Null}"/>
        <Label x:Name="lblGOT" Content="GOT Quote Generator" HorizontalAlignment="Left" Height="76" Margin="227,42,0,0" VerticalAlignment="Top" Width="335" FontSize="24" FontFamily="Trajan Pro" Grid.RowSpan="2"/>

    </Grid>
</Window>
